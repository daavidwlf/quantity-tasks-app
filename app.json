{
  "expo": {
    "name": "Household",
    "slug": "household",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-house.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-house.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-house.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon-house.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./data/fonts/ttf/Supreme-Variable.ttf"]
        }
      ]
    ]
  }
}
